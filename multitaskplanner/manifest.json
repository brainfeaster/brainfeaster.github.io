{
    "manifest_version": 2,
    "name": "ComPlan Extension",
    "short_name": "2 hour session management.",
    "description": " manage every 2 hours of your time.",
    "version": "1.0.0",
    "author": "Brainfeaster@github.com",
    "icons": {
        "19": "Icon-19.png",
        "38": "Icon-38.png",
        "48": "Icon-48.png",
        "128": "Icon-128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "Icon-19.png",
            "38": "Icon-38.png"
        },
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": ["assets/css/*","assets/js/*","assets/fonts/*"],
    "permissions": ["activeTab","storage"]
}